{
  "name": "stryker-git-checker",
  "version": "0.1.1",
  "description": "Git Checker for Stryker Mutator Javascript testing framework",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "git@github.com:lbtoma/stryker-git-checker.git",
  "engines": {
    "node": ">=12"
  },
  "author": "Lucas Barioni Toma <lbtoma@gmail.com>",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "test:unit": "jest",
    "lint:fix": "yarn prettier --end-of-line=lf --ignore-path .gitignore --write -c .",
    "lint:check": "yarn prettier --end-of-line=lf --ignore-path .gitignore -c .",
    "release": "rm -fr dist/ && yarn build && yarn publish"
  },
  "dependencies": {
    "@stryker-mutator/api": "^5.3.0",
    "typed-inject": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
